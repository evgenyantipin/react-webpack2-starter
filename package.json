{
  "name": "react-webpack2-starter",
  "version": "1.0.0",
  "description": "React and Webpack2 starter",
  "main": "index.js",
  "engines": {
    "node": ">=5.0 <8",
    "npm": ">=3.0 <5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jekaantipin/react-webpack2-starter"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "dev": "node webpack-dev-server.js",
    "build": "npm run clean && NODE_ENV=production ./node_modules/webpack/bin/webpack.js --define process.env.NODE_ENV='\"production\"' --config ./webpack.prod.config.js --display-error-details"
  },
  "keywords": [
    "react",
    "webpack 2",
    "code spltting",
    "express",
    "es6",
    "flow",
    "starter kit",
    "boilerpate",
    "hot reloading",
    "babel",
    "postcss"
  ],
  "author": "Evgeny Antipin",
  "license": "ISC",
  "dependencies": {
    "express": "^4.15.0",
    "babel-core": "^6.23.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel": "^6.23.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
